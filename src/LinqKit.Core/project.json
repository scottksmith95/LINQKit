{
    "version": "1.2.0.0",
    "title": "LinqKit",
    "description": "LinqKit.Core is a free set of extensions for LINQ to SQL and Entity Framework power users. Note that IAsync and IDbAsync are not supported.",
    "authors": [ "Joseph Albahari", "Tomas Petricek", "Scott Smith", "Tuomas Hietanen", "Stef Heyenrath" ],

    "packOptions": {
        "summary": "LinqKit.Core is a free set of extensions for LINQ to SQL and Entity Framework power users. Note that IAsync and IDbAsync are not supported. For that you need LinqKit.EntityFramework or LinqKit.Microsoft.EntityFrameworkCore.",
        "tags": [ "linq", "EF", "EntityFramework", "Entity", "Framework" ],
        "owners": [ "San Chen", "Scott Smith", "Tuomas Hietanen", "Stef Heyenrath" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/scottksmith95/LINQKit"
        },
        "projectUrl": "https://github.com/scottksmith95/LINQKit",
        "licenseUrl": "https://github.com/scottksmith95/LINQKit/master/license.txt",
        "releaseNotes": ""
    },

    "buildOptions": {
        "xmlDoc": true,
        "define": [ "NOEF" ],
        "keyFile": "../LinqKit.Net45/LinqKit.snk"
    },

