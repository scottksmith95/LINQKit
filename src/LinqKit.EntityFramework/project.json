{
    "version": "1.0.3.1",
    "title": "LinqKit for EntityFramework (with IAsync support)",
    "description": "LinqKit.EntityFramework is a free set of extensions for LINQ to SQL and Entity Framework power users. (IAsync supported)",
    "authors": [ "Joseph Albahari", "Tomas Petricek", "Scott Smith", "Tuomas Hietanen", "Stef Heyenrath" ],

    "packOptions": {
        "summary": "LinqKit.EntityFramework is a free set of extensions for LINQ to SQL and Entity Framework power users. (IAsync supported)",
        "tags": [ "linq", "EF", "EntityFramework", "Entity", "Framework" ],
        "owners": [ "Stef Heyenrath" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/scottksmith95/LINQKit"
        },
        "projectUrl": "https://github.com/scottksmith95/LINQKit",
        "licenseUrl": "https://github.com/scottksmith95/LINQKit/master/license.txt",
        "releaseNotes": "Fixed UWP Release mode issue."
    },

    "buildOptions": {
        "xmlDoc": true,
        "define": [ "EF" ],
        "compile": {
            "include": [
                "../LinqKit.Core/*.cs",
                "../LinqKit.Core/*/*.cs"
            ]
        },
        "keyFile": "LinqKit.EntityFramework.snk"
    },

    "dependencies": {
        "JetBrains.Annotations": {
            "version": "10.0.0",
            "type": "build"
        },
        "EntityFramework": "6.1.3"
    },

    "frameworks": {
        "net45": {
        },
        "net451": {
        },
        "net452": {
        },
        "net46": {
        },
        "net461": {
        }
    }
}